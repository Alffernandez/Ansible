# For possible options see https://docs.ansible.com/ansible/latest/collections/awx/awx/job_template_module.html#ansible-collections-awx-awx-job-template-module
ansible_tower_config:
  default:
    job_templates:
      DBA_oracle_internal_audit_to_splunk:  &DBA_oracle_internal_audit_to_splunk
        description: Parses internal audit files and sends them to Splunk
        inventory: DBA_ansible_unix_inventory
        limit: 'oracle:!lond*:!lont*:!dv*:!ts*' # This is further reduced in the playbook to only Prod
        credentials:
          - MACHINE_z_unixadm_prd
        concurrent_jobs_enabled: False 
        project: DBA_ansible_unix_apps
        playbook: playbooks/oracle_audit_splunk/pull_oracle_internal_audit.yml
        become_enabled: True

      DBA_rubrik_generic_backup:  &DBA_rubrik_generic_backup
        description: Rubrik Generic Oracle RMAN Backup, these can be setup as Full/Archive backups
        inventory: DBA_ansible_unix_inventory
        credentials:
          - MACHINE_DBA_z_rubrikbackup_prd
          - VAULT_DBA_rubrik_backups
        concurrent_jobs_enabled: False 
        ask_limit: True 
        project: DBA_rubrik_backups
        playbook: oracle_rman_rubrik.yml
        become_enabled: False
        
      DBA_rubrik_generic_adhoc_backup:  &DBA_rubrik_generic_adhoc_backup
        description: Rubrik Generic Ad-hoc Backup
        inventory: DBA_ansible_unix_inventory
        credentials:
          - MACHINE_DBA_z_rubrikbackup_prd
          - VAULT_DBA_rubrik_backups
        concurrent_jobs_enabled: True 
        ask_limit: True 
        project: DBA_rubrik_backups
        extra_vars:
          level: 1
          channels: 4
          maxsize: 16
          retention: 14
          special: A
          rubrik_version: 5.0
        playbook: oracle_rman_rubrik.yml
        become_enabled: False
        survey_enabled: True
        survey_spec:
          name: ''
          description: ''
          survey_enabled: True
          spec:
            - question_name: Database Backup Level
              question_description: "database level backup required (0..4); ignored for backup type=A"
              min: 0
              max: 4            
              required: True
              type: integer
              new_question: true
              variable: level
              default: 1
            - question_name: Backup Retention
              question_description: "backup retention in days ( default-> 14 )"
              min: 0
              max: 9999            
              required: True
              type: integer
              new_question: true
              variable: retention
              default: 14
            - question_name: Type of Backup
              question_description: "A -> archivelog or F -> full"
              min: 0
              max: 9999            
              required: True
              type: multiplechoice
              new_question: true
              variable: special
              default: A
              choices:
                - A
                - F
            - question_name: CHANNELS
              question_description: "how many backup channels ( 1..4 )"
              min: 1
              max: 4            
              required: True
              type: integer
              new_question: true
              variable: channels
              default: 2
            - question_name: Ignore Open volume?
              question_description: "Does not try to make the volume writable, use carefully!"
              required: True
              type: multiplechoice
              new_question: true
              variable: ignore_open_volume
              default: 'False'
              choices:
                - 'False'
                - 'True'



